{
  "DotLoading": "<template>\n  <div id=\"dot-Loading\" class=\"dot-loading\">\n    <div\n      v-for=\"i in dotAmount\"\n      :id=\"`child${i - 1}`\"\n      :key=\"i\"\n      class=\"loading-child\"\n      :class=\"i\"\n    ></div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport Vue from 'vue'\nexport default Vue.extend({\n  name: 'DotLoading',\n  props: {\n    dotAmount: {\n      type: Number,\n      default: 5,\n    },\n  },\n  mounted() {\n    for (let index = 0; index < this.dotAmount; index++) {\n      const element = document.getElementById(`child${index}`)\n      element?.style.setProperty(\n        'animation-delay',\n        `${(1.5 * (index + 1)) / this.dotAmount}s`\n      )\n    }\n  },\n})\n</script>\n\n<style scoped lang=\"scss\">\n.dot-loading {\n  padding: 20px;\n  @include flexR;\n  .loading-child {\n    width: 10px;\n    height: 10px;\n    background-color: $background;\n    margin-inline: 5px;\n    border-radius: 50%;\n    display: inline-block;\n    animation: loading-bounce 1.5s ease-in-out 0s infinite both;\n  }\n}\n@keyframes loading-bounce {\n  0%,\n  80%,\n  100% {\n    transform: scale(0);\n  }\n  40% {\n    transform: scale(1);\n  }\n}\n</style>\n",
  "LineLoading": "<template>\n  <div \n    class=\"line-loading\"\n    :class=\"{active: hasStop}\"\n  ></div>\n</template>\n\n<script lang=\"ts\">\nimport Vue from 'vue'\nexport default Vue.extend({\n  name: 'LineLoading',\n  props: {\n    hasStop: {\n      type: Boolean,\n      default: false,\n    }\n  },\n})\n</script>\n\n<style scoped lang=\"scss\">\n.line-loading {\n  width: 200px;\n  height: 3px;\n  margin: 20px 10px;\n  position: relative;\n  overflow: hidden;\n  background-color: $background;\n  border-radius: 20px;\n}\n.line-loading.active:before {\n  content: '';\n  position: absolute;\n  left: -50%;\n  height: 3px;\n  width: 40%;\n  background-color: $primary;\n  animation: lineMoveStop 1.6s linear infinite;\n  border-radius: 20px;\n}\n.line-loading:before {\n  content: '';\n  position: absolute;\n  left: -50%;\n  height: 3px;\n  width: 40%;\n  background-color: $primary;\n  animation: lineMove 1.6s linear infinite;\n  border-radius: 20px;\n}\n@keyframes lineMoveStop{\n  0% {\n    left: -40%;\n  }\n  50% {\n    left: 40%;\n    width: 40%;\n  }\n  65% {\n    left: 40%;\n    width: 40%;\n  }\n  100% {\n    left: 100%;\n    width: 100%;\n  }\n}\n@keyframes lineMove {\n  0% {\n    left: -40%;\n  }\n  50% {\n    left: 20%;\n    width: 80%;\n  }\n  100% {\n    left: 100%;\n    width: 100%;\n  }\n}\n</style>\n",
  "StatusLoading": "<template>\n  <div class=\"status-loading\">\n    <div class=\"loader\" :class=\"status\"></div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport Vue from 'vue'\nexport default Vue.extend({\n  name: 'StatusLoading',\n  props: {\n    status: {\n      type: String,\n      default: 'loading',\n    },\n  },\n})\n</script>\n\n<style scoped lang=\"scss\">\n.status-loading {\n  position: relative;\n  height: 80px;\n  width: 80px;\n  margin: 10px;\n}\n.loader {\n  position: absolute;\n  content: '';\n  width: 80px;\n  height: 80px;\n  border: 5px solid $background;\n  border-radius: 50%;\n  overflow: hidden;\n  transition: 0.3s;\n  animation: spin 1s linear infinite;\n}\n.success {\n  background: $background;\n  border-color: $background;\n}\n.fail {\n  background: $background;\n  border-color: $background;\n}\n.loader.success {\n  &::before {\n    height: 7px;\n    width: 2.5rem;\n    position: absolute;\n    top: 65%;\n    left: 44%;\n    background-color: $success;\n    transform: rotate(-45deg);\n    transform-origin: 0% 50%;\n    border-radius: 5px;\n    animation: baseGrow 1s;\n    content: '';\n  }\n\n  &::after {\n    height: 7px;\n    width: 1.5rem;\n    position: absolute;\n    top: 65%;\n    left: 50%;\n    background-color: $success;\n    transform: rotate(-135deg);\n    transform-origin: 0% 50%;\n    border-radius: 5px;\n    animation: tipGrow 1s;\n    content: '';\n  }\n}\n.loader.fail {\n  &::before {\n    width: 3rem;\n    height: 0.5rem;\n    background: $error;\n    transform-origin: 50% 50%;\n    top: calc(50% - 0.25rem);\n    left: calc(50% - 1.5rem);\n    transform: rotate(45deg);\n    position: absolute;\n    content: '';\n    border-radius: 10px;\n    animation: leftIn 0.4s linear;\n    content: '';\n  }\n\n  &::after {\n    width: 3rem;\n    height: 0.5rem;\n    background: $error;\n    transform-origin: 50% 50%;\n    top: calc(50% - 0.25rem);\n    right: calc(50% - 1.5rem);\n    transform: rotate(-45deg);\n    position: absolute;\n    content: '';\n    border-radius: 10px;\n    animation: rightIn 0.4s linear;\n    content: '';\n  }\n}\n.loader.success,\n.loader.fail {\n  animation: pop 1.2s ease-in-out;\n}\n\n@keyframes spin {\n  0% {\n    transform: rotate(0deg);\n    border-top: 5px solid $primary;\n  }\n  100% {\n    transform: rotate(360deg);\n    border-top: 5px solid $primary;\n  }\n}\n\n@keyframes tipGrow {\n  0% {\n    width: 0px;\n    left: 0;\n    top: 0;\n  }\n  25% {\n    width: 0px;\n    left: 0;\n    top: 0;\n  }\n  50% {\n    top: 0%;\n    left: 0%;\n    width: 0;\n  }\n  75% {\n    top: 0%;\n    left: 0%;\n    width: 0rem;\n  }\n  100% {\n    top: 65%;\n    left: 50%;\n    width: 1.5rem;\n  }\n}\n\n@keyframes baseGrow {\n  0% {\n    width: 0;\n  }\n  90% {\n    width: 0;\n  }\n  100% {\n    width: 2.5rem;\n  }\n}\n\n@keyframes pop {\n  0% {\n    transform: scale(1);\n  }\n  70% {\n    transform: scale(1);\n  }\n  100% {\n    transform: scale(1.1);\n  }\n}\n\n@keyframes leftIn {\n  0% {\n    left: 0;\n    top: 0;\n    width: 0;\n  }\n  100% {\n    top: calc(50% - 0.25rem);\n    left: calc(50% - 1.5rem);\n    width: 3rem;\n  }\n}\n\n@keyframes rightIn {\n  0% {\n    right: 0;\n    top: 0;\n    width: 0;\n  }\n  100% {\n    top: calc(50% - 0.25rem);\n    right: calc(50% - 1.5rem);\n    width: 3rem;\n  }\n}\n</style>\n"
}
