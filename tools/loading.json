{
  "DotLoading": "<template>\r\n  <div id=\"dot-Loading\" class=\"dot-loading\">\r\n    <div\r\n      v-for=\"i in dotAmount\"\r\n      :id=\"`child${i - 1}`\"\r\n      :key=\"i\"\r\n      class=\"loading-child\"\r\n      :class=\"i\"\r\n    ></div>\r\n  </div>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from 'vue'\r\nexport default Vue.extend({\r\n  name: 'DotLoading',\r\n  props: {\r\n    dotAmount: {\r\n      type: Number,\r\n      default: 5,\r\n    },\r\n  },\r\n  mounted() {\r\n    for (let index = 0; index < this.dotAmount; index++) {\r\n      const element = document.getElementById(`child${index}`)\r\n      element?.style.setProperty(\r\n        'animation-delay',\r\n        `${(1.5 * (index + 1)) / this.dotAmount}s`\r\n      )\r\n    }\r\n  },\r\n})\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.dot-loading {\r\n  padding: 20px;\r\n  @include flexR;\r\n  .loading-child {\r\n    width: 10px;\r\n    height: 10px;\r\n    background-color: $background;\r\n    margin-inline: 5px;\r\n    border-radius: 50%;\r\n    display: inline-block;\r\n    animation: loading-bounce 1.5s ease-in-out 0s infinite both;\r\n  }\r\n}\r\n@keyframes loading-bounce {\r\n  0%,\r\n  80%,\r\n  100% {\r\n    transform: scale(0);\r\n  }\r\n  40% {\r\n    transform: scale(1);\r\n  }\r\n}\r\n</style>\r\n",
  "LineLoading": "<template>\r\n  <div class=\"line-loading\" :class=\"{ active: hasStop }\"></div>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from 'vue'\r\nexport default Vue.extend({\r\n  name: 'LineLoading',\r\n  props: {\r\n    hasStop: {\r\n      type: Boolean,\r\n      default: false,\r\n    },\r\n  },\r\n})\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.line-loading {\r\n  width: 200px;\r\n  height: 3px;\r\n  margin: 20px 10px;\r\n  position: relative;\r\n  overflow: hidden;\r\n  background-color: $background;\r\n  border-radius: 20px;\r\n}\r\n.line-loading.active:before {\r\n  content: '';\r\n  position: absolute;\r\n  left: -50%;\r\n  height: 3px;\r\n  width: 40%;\r\n  background-color: $primary;\r\n  animation: lineMoveStop 1.6s linear infinite;\r\n  border-radius: 20px;\r\n}\r\n.line-loading:before {\r\n  content: '';\r\n  position: absolute;\r\n  left: -50%;\r\n  height: 3px;\r\n  width: 40%;\r\n  background-color: $primary;\r\n  animation: lineMove 1.6s linear infinite;\r\n  border-radius: 20px;\r\n}\r\n@keyframes lineMoveStop {\r\n  0% {\r\n    left: -40%;\r\n  }\r\n  50% {\r\n    left: 40%;\r\n    width: 40%;\r\n  }\r\n  65% {\r\n    left: 40%;\r\n    width: 40%;\r\n  }\r\n  100% {\r\n    left: 100%;\r\n    width: 100%;\r\n  }\r\n}\r\n@keyframes lineMove {\r\n  0% {\r\n    left: -40%;\r\n  }\r\n  50% {\r\n    left: 20%;\r\n    width: 80%;\r\n  }\r\n  100% {\r\n    left: 100%;\r\n    width: 100%;\r\n  }\r\n}\r\n</style>\r\n",
  "StatusLoading": "<template>\r\n  <div class=\"status-loading\">\r\n    <div class=\"loader\" :class=\"status\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from 'vue'\r\nexport default Vue.extend({\r\n  name: 'StatusLoading',\r\n  props: {\r\n    status: {\r\n      type: String,\r\n      default: 'loading',\r\n    },\r\n  },\r\n})\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.status-loading {\r\n  position: relative;\r\n  height: 80px;\r\n  width: 80px;\r\n  margin: 10px;\r\n}\r\n.loader {\r\n  position: absolute;\r\n  content: '';\r\n  width: 80px;\r\n  height: 80px;\r\n  border: 5px solid $background;\r\n  border-radius: 50%;\r\n  overflow: hidden;\r\n  transition: 0.3s;\r\n  animation: spin 1s linear infinite;\r\n}\r\n.success {\r\n  background: $background;\r\n  border-color: $background;\r\n}\r\n.fail {\r\n  background: $background;\r\n  border-color: $background;\r\n}\r\n.loader.success {\r\n  &::before {\r\n    height: 7px;\r\n    width: 2.5rem;\r\n    position: absolute;\r\n    top: 65%;\r\n    left: 44%;\r\n    background-color: $success;\r\n    transform: rotate(-45deg);\r\n    transform-origin: 0% 50%;\r\n    border-radius: 5px;\r\n    animation: baseGrow 1s;\r\n    content: '';\r\n  }\r\n\r\n  &::after {\r\n    height: 7px;\r\n    width: 1.5rem;\r\n    position: absolute;\r\n    top: 65%;\r\n    left: 50%;\r\n    background-color: $success;\r\n    transform: rotate(-135deg);\r\n    transform-origin: 0% 50%;\r\n    border-radius: 5px;\r\n    animation: tipGrow 1s;\r\n    content: '';\r\n  }\r\n}\r\n.loader.fail {\r\n  &::before {\r\n    width: 3rem;\r\n    height: 0.5rem;\r\n    background: $error;\r\n    transform-origin: 50% 50%;\r\n    top: calc(50% - 0.25rem);\r\n    left: calc(50% - 1.5rem);\r\n    transform: rotate(45deg);\r\n    position: absolute;\r\n    content: '';\r\n    border-radius: 10px;\r\n    animation: leftIn 0.4s linear;\r\n    content: '';\r\n  }\r\n\r\n  &::after {\r\n    width: 3rem;\r\n    height: 0.5rem;\r\n    background: $error;\r\n    transform-origin: 50% 50%;\r\n    top: calc(50% - 0.25rem);\r\n    right: calc(50% - 1.5rem);\r\n    transform: rotate(-45deg);\r\n    position: absolute;\r\n    content: '';\r\n    border-radius: 10px;\r\n    animation: rightIn 0.4s linear;\r\n    content: '';\r\n  }\r\n}\r\n.loader.success,\r\n.loader.fail {\r\n  animation: pop 1.2s ease-in-out;\r\n}\r\n\r\n@keyframes spin {\r\n  0% {\r\n    transform: rotate(0deg);\r\n    border-top: 5px solid $primary;\r\n  }\r\n  100% {\r\n    transform: rotate(360deg);\r\n    border-top: 5px solid $primary;\r\n  }\r\n}\r\n\r\n@keyframes tipGrow {\r\n  0% {\r\n    width: 0px;\r\n    left: 0;\r\n    top: 0;\r\n  }\r\n  25% {\r\n    width: 0px;\r\n    left: 0;\r\n    top: 0;\r\n  }\r\n  50% {\r\n    top: 0%;\r\n    left: 0%;\r\n    width: 0;\r\n  }\r\n  75% {\r\n    top: 0%;\r\n    left: 0%;\r\n    width: 0rem;\r\n  }\r\n  100% {\r\n    top: 65%;\r\n    left: 50%;\r\n    width: 1.5rem;\r\n  }\r\n}\r\n\r\n@keyframes baseGrow {\r\n  0% {\r\n    width: 0;\r\n  }\r\n  90% {\r\n    width: 0;\r\n  }\r\n  100% {\r\n    width: 2.5rem;\r\n  }\r\n}\r\n\r\n@keyframes pop {\r\n  0% {\r\n    transform: scale(1);\r\n  }\r\n  70% {\r\n    transform: scale(1);\r\n  }\r\n  100% {\r\n    transform: scale(1.1);\r\n  }\r\n}\r\n\r\n@keyframes leftIn {\r\n  0% {\r\n    left: 0;\r\n    top: 0;\r\n    width: 0;\r\n  }\r\n  100% {\r\n    top: calc(50% - 0.25rem);\r\n    left: calc(50% - 1.5rem);\r\n    width: 3rem;\r\n  }\r\n}\r\n\r\n@keyframes rightIn {\r\n  0% {\r\n    right: 0;\r\n    top: 0;\r\n    width: 0;\r\n  }\r\n  100% {\r\n    top: calc(50% - 0.25rem);\r\n    right: calc(50% - 1.5rem);\r\n    width: 3rem;\r\n  }\r\n}\r\n</style>\r\n"
}
